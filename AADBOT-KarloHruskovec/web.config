<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<httpProtocol>
			<customHeaders>
				<remove name="X-Powered-By" />
				<remove name="Server" />

				<add name="Strict-Transport-Security" value="max-age=31536000; includeSubDomains; preload" />
			</customHeaders>
		</httpProtocol>
	</system.webServer>
</configuration>
